generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model deleted_msg {
  id          Int      @id(map: "deletedmsgs_pkey") @default(autoincrement())
  author      String
  content     String
  guildId     String
  msgTime     DateTime @db.Timestamp(6)
  channel     String
  deletedTime DateTime @db.Timestamp(6)
  deletedBy   String   @db.VarChar(200)
  msgId       String   @unique(map: "deletedmsgs_msgid_key")
  attachments Json?
  member      member   @relation(fields: [author, guildId], references: [userid, guild], onDelete: Cascade, onUpdate: Cascade, map: "deletedmsgs_author_guildid_fkey")
}

model guild {
  id         Int      @id(map: "guilds_pkey") @default(autoincrement())
  guildId    String   @unique(map: "guilds_guildid_key")
  joinedTime DateTime @db.Timestamp(6)
  prefix     String   @default("g")
  disabled   String[]
  member     member[]
}

model member {
  id                  Int           @id(map: "members_pkey") @default(autoincrement())
  guild               String
  userid              String
  guild_guildTomember guild         @relation(fields: [guild], references: [guildId], onDelete: Cascade, onUpdate: Cascade, map: "members_guild_fkey")
  deleted_msg         deleted_msg[]
  member_level        member_level?

  @@unique([guild, userid], map: "members_guild_userid_key")
}

model scheduled_task {
  id      Int      @id(map: "scheduled_tasks_pkey") @default(autoincrement())
  task    String   @db.VarChar(255)
  context Json?    @db.Json
  time    DateTime @db.Timestamp(6)
  done    Boolean  @default(false)
}

model member_level {
  memberID                    String
  guildID                     String
  level                       Int    @default(0)
  nextLevelXp                 BigInt @default(1000)
  xp                          BigInt @default(0)
  member_memberTomember_level member @relation(fields: [memberID, guildID], references: [userid, guild], onDelete: Cascade)
  @@unique([memberID, guildID])
}
